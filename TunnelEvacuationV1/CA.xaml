<Window x:Class="TunnelEvacuationV1.CA"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TunnelEvacuationV1"
        mc:Ignorable="d"
        Title="CA" Height="650" Width="800">
    <StackPanel>
    <ScrollViewer HorizontalScrollBarVisibility="Auto">
        <Image VerticalAlignment="Top" HorizontalAlignment="Left" x:Name="stack">
        </Image> 
        
    </ScrollViewer>
        <Button  Name="Start" Content="Rozpocznij symulacje" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="10" Margin="15"/>
    </StackPanel>
</Window>
